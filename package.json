{
  "name": "warframetoolchatbot",
  "version": "0.0.1",
  "description": "A chat bot for twitch that provides useful commands to query Warframe information",
  "main": "dist/bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --copy-files",
    "start": "node dist/bot.js "
  },
  "nodemonConfig": {
    "events": {
      "restart": "npm run build"
    },
    "watch": [
      "src/",
      "dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iBelg/warframetoolchatbot.git"
  },
  "keywords": [
    "twitch",
    "chat",
    "bot",
    "chatbot",
    "warframe"
  ],
  "author": "iBelg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iBelg/warframetoolchatbot/issues"
  },
  "homepage": "https://github.com/iBelg/warframetoolchatbot#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-js-logger": "^1.0.17",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^5.0.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "js-logger": "^1.4.1",
    "lodash": "^4.17.10",
    "ws": "^5.1.1"
  }
}
